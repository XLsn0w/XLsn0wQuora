
platform :ios, '9.0'

use_frameworks! #由于是Swift(objc不需要), 我们这里还需要换行输入use_frameworks!  这样才能将对应的库导入到我们的工程中。

target "XLsn0wQuora" do
    
    pod 'XLsn0wKit_swift'
    
    pod 'SnapKit'    #纯代码布局库
    pod 'Kingfisher' #图片缓存库
    pod 'Alamofire'  #网络解析库
    
    pod 'SQLite.swift'
    
	pod 'SwiftyJSON'
    pod 'HandyJSON'
    
    pod 'Moya'
    pod 'Moya/RxSwift'
    
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxDataSources'
    
    pod 'SwiftDate'
    pod 'Then'
    
    #objc Kit
    pod 'FMDB'
    
end
